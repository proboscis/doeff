{
  "name": "doeff-runner",
  "displayName": "doeff runner",
  "description": "Run doeff Programs from VS Code using the doeff-indexer and Python debugger.",
  "version": "0.6.20",
  "publisher": "Proboscis",
  "icon": "icon.png",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/proboscis/doeff.git"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onCommand:doeff-runner.runDefault",
    "onCommand:doeff-runner.runOptions",
    "onCommand:doeff-runner.runConfig",
    "onCommand:doeff-runner.runWithKleisli",
    "onCommand:doeff-runner.runWithTransform",
    "onCommand:doeff-runner.showMoreKleisli",
    "onCommand:doeff-runner.showMoreTransforms",
    "onCommand:doeff-runner.toggleDebugMode",
    "onCommand:doeff-runner.addToPlaylist",
    "onCommand:doeff-runner.pickProgram",
    "onCommand:doeff-runner.pickAndRun",
    "onCommand:doeff-runner.pickAndAddToPlaylist",
    "onCommand:doeff-runner.pickPlaylist",
    "onCommand:doeff-runner.pickPlaylistItem",
    "onCommand:doeff-runner.pickAndRunPlaylistItem",
    "onCommand:doeff-runner.runPlaylistItem",
    "onCommand:doeff-runner.revealPlaylistItem",
    "onCommand:doeff-runner.editPlaylistItem",
    "onCommand:doeff-runner.removePlaylistItem",
    "onCommand:doeff-runner.createPlaylist",
    "onCommand:doeff-runner.renamePlaylist",
    "onCommand:doeff-runner.deletePlaylist",
    "onCommand:doeff-runner.openPlaylistsFile",
    "onCommand:doeff-runner.setPlaylistsSortMode",
    "onCommand:doeff-runner.addCustomPlaylistItem",
    "onCommand:doeff-runner.copyPlaylistItemPath",
    "onCommand:doeff-runner.listWorkflows",
    "onCommand:doeff-runner.attachWorkflow",
    "onCommand:doeff-runner.watchWorkflow",
    "onCommand:doeff-runner.stopWorkflow",
    "onCommand:doeff-runner.refreshWorkflows",
    "onView:doeff-programs",
    "onView:doeff-programs-all",
    "onView:doeff-playlists",
    "onView:doeff-workflows"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "doeff-explorer",
          "title": "doeff",
          "icon": "$(symbol-method)"
        }
      ]
    },
    "views": {
      "doeff-explorer": [
        {
          "id": "doeff-programs",
          "name": "Programs"
        },
        {
          "id": "doeff-programs-all",
          "name": "Programs (All Worktrees)"
        },
        {
          "id": "doeff-playlists",
          "name": "Playlists"
        },
        {
          "id": "doeff-workflows",
          "name": "Workflows"
        }
      ]
    },
    "commands": [
      {
        "command": "doeff-runner.runDefault",
        "title": "Run doeff Program (default)"
      },
      {
        "command": "doeff-runner.runOptions",
        "title": "Run doeff Program (select options)"
      },
      {
        "command": "doeff-runner.runConfig",
        "title": "Run doeff Selection"
      },
      {
        "command": "doeff-runner.runWithKleisli",
        "title": "Run doeff Program with Kleisli"
      },
      {
        "command": "doeff-runner.runWithTransform",
        "title": "Run doeff Program with Transform"
      },
      {
        "command": "doeff-runner.showMoreKleisli",
        "title": "Show More Kleisli Tools"
      },
      {
        "command": "doeff-runner.showMoreTransforms",
        "title": "Show More Transform Tools"
      },
      {
        "command": "doeff-runner.refreshExplorer",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "doeff-runner.revealEntrypoint",
        "title": "Go to Definition"
      },
      {
        "command": "doeff-runner.runFromTree",
        "title": "Run"
      },
      {
        "command": "doeff-runner.setDefaultAction",
        "title": "Set as Default",
        "icon": "$(star)"
      },
      {
        "command": "doeff-runner.clearDefaultAction",
        "title": "Clear Default Action",
        "icon": "$(star-empty)"
      },
      {
        "command": "doeff-runner.searchEntrypoints",
        "title": "Search Entrypoints",
        "icon": "$(search)"
      },
      {
        "command": "doeff-runner.filterEntrypoints",
        "title": "Filter Entrypoints",
        "icon": "$(filter)"
      },
      {
        "command": "doeff-runner.clearFilter",
        "title": "Clear Filter",
        "icon": "$(close)"
      },
      {
        "command": "doeff-runner.toggleDebugMode",
        "title": "Toggle Debug Mode",
        "icon": "$(bug)"
      },
      {
        "command": "doeff-runner.inspectEnvKey",
        "title": "Inspect Environment Key",
        "icon": "$(search)"
      },
      {
        "command": "doeff-runner.resolveEnvKey",
        "title": "Resolve Key at Runtime",
        "icon": "$(play)"
      },
      {
        "command": "doeff-runner.addToPlaylist",
        "title": "Add doeff Program to Playlist"
      },
      {
        "command": "doeff-runner.pickProgram",
        "title": "Pick doeff Program (All Worktrees)"
      },
      {
        "command": "doeff-runner.pickAndRun",
        "title": "Pick and Run doeff Program (All Worktrees)"
      },
      {
        "command": "doeff-runner.pickAndAddToPlaylist",
        "title": "Pick and Add doeff Program to Playlist"
      },
      {
        "command": "doeff-runner.pickPlaylist",
        "title": "Pick Playlist"
      },
      {
        "command": "doeff-runner.pickPlaylistItem",
        "title": "Pick Playlist Item"
      },
      {
        "command": "doeff-runner.pickAndRunPlaylistItem",
        "title": "Pick and Run Playlist Item"
      },
      {
        "command": "doeff-runner.runPlaylistItem",
        "title": "Run Playlist Item",
        "icon": "$(play)"
      },
      {
        "command": "doeff-runner.revealPlaylistItem",
        "title": "Go to Definition (Playlist Item)"
      },
      {
        "command": "doeff-runner.editPlaylistItem",
        "title": "Edit Playlist Item"
      },
      {
        "command": "doeff-runner.removePlaylistItem",
        "title": "Remove Playlist Item"
      },
      {
        "command": "doeff-runner.createPlaylist",
        "title": "Create Playlist"
      },
      {
        "command": "doeff-runner.renamePlaylist",
        "title": "Rename Playlist"
      },
      {
        "command": "doeff-runner.deletePlaylist",
        "title": "Delete Playlist"
      },
      {
        "command": "doeff-runner.openPlaylistsFile",
        "title": "Open Playlists File"
      },
      {
        "command": "doeff-runner.setPlaylistsSortMode",
        "title": "Playlists: Set Item Order"
      },
      {
        "command": "doeff-runner.addPlaylistItemWorktreeToWorkspace",
        "title": "Add Worktree to Workspace",
        "icon": "$(folder-library)"
      },
      {
        "command": "doeff-runner.addCustomPlaylistItem",
        "title": "Add Custom Command to Playlist",
        "icon": "$(add)"
      },
      {
        "command": "doeff-runner.copyPlaylistItemPath",
        "title": "Copy Path",
        "icon": "$(copy)"
      },
      {
        "command": "doeff-runner.listWorkflows",
        "title": "Doeff: List Workflows"
      },
      {
        "command": "doeff-runner.attachWorkflow",
        "title": "Doeff: Attach to Workflow",
        "icon": "$(terminal)"
      },
      {
        "command": "doeff-runner.watchWorkflow",
        "title": "Doeff: Watch Workflow",
        "icon": "$(eye)"
      },
      {
        "command": "doeff-runner.stopWorkflow",
        "title": "Doeff: Stop Workflow",
        "icon": "$(debug-stop)"
      },
      {
        "command": "doeff-runner.refreshWorkflows",
        "title": "Refresh Workflows",
        "icon": "$(refresh)"
      }
    ],
    "configuration": {
      "title": "doeff Runner",
      "properties": {
        "doeff-runner.envInspector.showUserConfig": {
          "type": "boolean",
          "default": true,
          "description": "Show ~/.doeff.py in environment chain"
        },
        "doeff-runner.envInspector.askTimeout": {
          "type": "number",
          "default": 10000,
          "description": "Timeout in milliseconds for runtime ask(key) execution"
        },
        "doeff-runner.envInspector.cacheEnvChain": {
          "type": "boolean",
          "default": true,
          "description": "Cache environment chain results"
        },
        "doeff-runner.envInspector.defaultCollapsed": {
          "type": "boolean",
          "default": true,
          "description": "Start environment chain nodes collapsed"
        },
        "doeff-runner.playlists.sortMode": {
          "type": "string",
          "enum": [
            "alpha",
            "manual"
          ],
          "default": "alpha",
          "description": "Playlist item order (alpha sorts by name; manual preserves file order and enables drag & drop sorting)."
        },
        "doeff-runner.worktrees.autoAddToWorkspace": {
          "type": "string",
          "enum": [
            "prompt",
            "always",
            "never"
          ],
          "default": "prompt",
          "description": "When opening entrypoints from other git worktrees, optionally add that worktree folder to the current workspace (enables features like Go to Definition)."
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "doeff-runner.toggleDebugMode",
          "when": "view == doeff-programs",
          "group": "navigation@0"
        },
        {
          "command": "doeff-runner.searchEntrypoints",
          "when": "view == doeff-programs",
          "group": "navigation@1"
        },
        {
          "command": "doeff-runner.filterEntrypoints",
          "when": "view == doeff-programs",
          "group": "navigation@2"
        },
        {
          "command": "doeff-runner.refreshExplorer",
          "when": "view == doeff-programs",
          "group": "navigation@3"
        },
        {
          "command": "doeff-runner.toggleDebugMode",
          "when": "view == doeff-programs-all",
          "group": "navigation@0"
        },
        {
          "command": "doeff-runner.pickProgram",
          "when": "view == doeff-programs-all",
          "group": "navigation@1"
        },
        {
          "command": "doeff-runner.refreshExplorer",
          "when": "view == doeff-programs-all",
          "group": "navigation@2"
        },
        {
          "command": "doeff-runner.toggleDebugMode",
          "when": "view == doeff-playlists",
          "group": "navigation@0"
        },
        {
          "command": "doeff-runner.createPlaylist",
          "when": "view == doeff-playlists",
          "group": "navigation@1"
        },
        {
          "command": "doeff-runner.openPlaylistsFile",
          "when": "view == doeff-playlists",
          "group": "navigation@2"
        },
        {
          "command": "doeff-runner.setPlaylistsSortMode",
          "when": "view == doeff-playlists",
          "group": "navigation@4"
        },
        {
          "command": "doeff-runner.refreshExplorer",
          "when": "view == doeff-playlists",
          "group": "navigation@3"
        },
        {
          "command": "doeff-runner.addCustomPlaylistItem",
          "when": "view == doeff-playlists",
          "group": "navigation@5"
        },
        {
          "command": "doeff-runner.refreshWorkflows",
          "when": "view == doeff-workflows",
          "group": "navigation@0"
        }
      ],
      "view/item/context": [
        {
          "command": "doeff-runner.addToPlaylist",
          "when": "view == doeff-programs && viewItem == entrypoint",
          "group": "inline"
        },
        {
          "command": "doeff-runner.addToPlaylist",
          "when": "view == doeff-programs-all && viewItem == worktreeEntrypoint",
          "group": "inline"
        },
        {
          "command": "doeff-runner.setDefaultAction",
          "when": "view == doeff-programs && viewItem == action",
          "group": "inline"
        },
        {
          "command": "doeff-runner.clearDefaultAction",
          "when": "view == doeff-programs && viewItem == entrypoint",
          "group": "inline"
        },
        {
          "command": "doeff-runner.revealEntrypoint",
          "when": "view == doeff-programs && viewItem == entrypoint",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.inspectEnvKey",
          "when": "(view == doeff-programs || view == doeff-programs-all || view == doeff-playlists) && viewItem == envChain",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.inspectEnvKey",
          "when": "(view == doeff-programs || view == doeff-programs-all || view == doeff-playlists) && viewItem == envSource",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.resolveEnvKey",
          "when": "(view == doeff-programs || view == doeff-programs-all || view == doeff-playlists) && viewItem == envKey",
          "group": "inline"
        },
        {
          "command": "doeff-runner.renamePlaylist",
          "when": "view == doeff-playlists && viewItem == playlist",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.deletePlaylist",
          "when": "view == doeff-playlists && viewItem == playlist",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.runPlaylistItem",
          "when": "view == doeff-playlists && viewItem == playlistItem",
          "group": "inline@0"
        },
        {
          "command": "doeff-runner.revealPlaylistItem",
          "when": "view == doeff-playlists && viewItem == playlistItem",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.addPlaylistItemWorktreeToWorkspace",
          "when": "view == doeff-playlists && viewItem == playlistItem",
          "group": "inline@1"
        },
        {
          "command": "doeff-runner.copyPlaylistItemPath",
          "when": "view == doeff-playlists && viewItem == playlistItem",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.runPlaylistItem",
          "when": "view == doeff-playlists && viewItem == customPlaylistItem",
          "group": "inline@0"
        },
        {
          "command": "doeff-runner.revealPlaylistItem",
          "when": "view == doeff-playlists && viewItem == customPlaylistItem",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.addPlaylistItemWorktreeToWorkspace",
          "when": "view == doeff-playlists && viewItem == customPlaylistItem",
          "group": "inline@1"
        },
        {
          "command": "doeff-runner.copyPlaylistItemPath",
          "when": "view == doeff-playlists && viewItem == customPlaylistItem",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.attachWorkflow",
          "when": "view == doeff-workflows && viewItem == workflow",
          "group": "inline@0"
        },
        {
          "command": "doeff-runner.watchWorkflow",
          "when": "view == doeff-workflows && viewItem == workflow",
          "group": "inline@1"
        },
        {
          "command": "doeff-runner.stopWorkflow",
          "when": "view == doeff-workflows && viewItem == workflow",
          "group": "inline@2"
        },
        {
          "command": "doeff-runner.attachWorkflow",
          "when": "view == doeff-workflows && viewItem == agent",
          "group": "inline@0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
