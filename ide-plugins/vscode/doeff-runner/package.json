{
  "name": "doeff-runner",
  "displayName": "doeff runner",
  "description": "Run doeff Programs from VS Code using the doeff-indexer and Python debugger.",
  "version": "0.5.2",
  "publisher": "Proboscis",
  "icon": "icon.png",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyberAgentAILab/doeff.git"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onCommand:doeff-runner.runDefault",
    "onCommand:doeff-runner.runOptions",
    "onCommand:doeff-runner.runConfig",
    "onCommand:doeff-runner.runWithKleisli",
    "onCommand:doeff-runner.runWithTransform",
    "onCommand:doeff-runner.showMoreKleisli",
    "onCommand:doeff-runner.showMoreTransforms",
    "onCommand:doeff-runner.toggleDebugMode",
    "onView:doeff-programs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "doeff-explorer",
          "title": "doeff",
          "icon": "$(symbol-method)"
        }
      ]
    },
    "views": {
      "doeff-explorer": [
        {
          "id": "doeff-programs",
          "name": "Programs"
        }
      ]
    },
    "commands": [
      {
        "command": "doeff-runner.runDefault",
        "title": "Run doeff Program (default)"
      },
      {
        "command": "doeff-runner.runOptions",
        "title": "Run doeff Program (select options)"
      },
      {
        "command": "doeff-runner.runConfig",
        "title": "Run doeff Selection"
      },
      {
        "command": "doeff-runner.runWithKleisli",
        "title": "Run doeff Program with Kleisli"
      },
      {
        "command": "doeff-runner.runWithTransform",
        "title": "Run doeff Program with Transform"
      },
      {
        "command": "doeff-runner.showMoreKleisli",
        "title": "Show More Kleisli Tools"
      },
      {
        "command": "doeff-runner.showMoreTransforms",
        "title": "Show More Transform Tools"
      },
      {
        "command": "doeff-runner.refreshExplorer",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "doeff-runner.revealEntrypoint",
        "title": "Go to Definition"
      },
      {
        "command": "doeff-runner.runFromTree",
        "title": "Run"
      },
      {
        "command": "doeff-runner.setDefaultAction",
        "title": "Set as Default",
        "icon": "$(star)"
      },
      {
        "command": "doeff-runner.clearDefaultAction",
        "title": "Clear Default Action",
        "icon": "$(star-empty)"
      },
      {
        "command": "doeff-runner.searchEntrypoints",
        "title": "Search Entrypoints",
        "icon": "$(search)"
      },
      {
        "command": "doeff-runner.filterEntrypoints",
        "title": "Filter Entrypoints",
        "icon": "$(filter)"
      },
      {
        "command": "doeff-runner.clearFilter",
        "title": "Clear Filter",
        "icon": "$(close)"
      },
      {
        "command": "doeff-runner.toggleDebugMode",
        "title": "Toggle Debug Mode",
        "icon": "$(bug)"
      },
      {
        "command": "doeff-runner.inspectEnvKey",
        "title": "Inspect Environment Key",
        "icon": "$(search)"
      },
      {
        "command": "doeff-runner.resolveEnvKey",
        "title": "Resolve Key at Runtime",
        "icon": "$(play)"
      }
    ],
    "configuration": {
      "title": "doeff Runner",
      "properties": {
        "doeff-runner.envInspector.showUserConfig": {
          "type": "boolean",
          "default": true,
          "description": "Show ~/.doeff.py in environment chain"
        },
        "doeff-runner.envInspector.askTimeout": {
          "type": "number",
          "default": 10000,
          "description": "Timeout in milliseconds for runtime ask(key) execution"
        },
        "doeff-runner.envInspector.cacheEnvChain": {
          "type": "boolean",
          "default": true,
          "description": "Cache environment chain results"
        },
        "doeff-runner.envInspector.defaultCollapsed": {
          "type": "boolean",
          "default": true,
          "description": "Start environment chain nodes collapsed"
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "doeff-runner.toggleDebugMode",
          "when": "view == doeff-programs",
          "group": "navigation@0"
        },
        {
          "command": "doeff-runner.searchEntrypoints",
          "when": "view == doeff-programs",
          "group": "navigation@1"
        },
        {
          "command": "doeff-runner.filterEntrypoints",
          "when": "view == doeff-programs",
          "group": "navigation@2"
        },
        {
          "command": "doeff-runner.refreshExplorer",
          "when": "view == doeff-programs",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "doeff-runner.setDefaultAction",
          "when": "view == doeff-programs && viewItem == action",
          "group": "inline"
        },
        {
          "command": "doeff-runner.clearDefaultAction",
          "when": "view == doeff-programs && viewItem == entrypoint",
          "group": "inline"
        },
        {
          "command": "doeff-runner.revealEntrypoint",
          "when": "view == doeff-programs && viewItem == entrypoint",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.inspectEnvKey",
          "when": "view == doeff-programs && viewItem == envChain",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.inspectEnvKey",
          "when": "view == doeff-programs && viewItem == envSource",
          "group": "navigation"
        },
        {
          "command": "doeff-runner.resolveEnvKey",
          "when": "view == doeff-programs && viewItem == envKey",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
