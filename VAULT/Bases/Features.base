filters: file.inFolder("Features")
properties:
  id:
    displayName: ID
  status:
    displayName: Status
  module:
    displayName: Module
  priority:
    displayName: Priority
views:
  - type: table
    name: All Features
    order:
      - id
      - status
      - module
      - priority
      - file.name
    sort:
      - property: file.basename
        direction: DESC
      - property: status
        direction: ASC
    columnSize:
      note.id: 144

